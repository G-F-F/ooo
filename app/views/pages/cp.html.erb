		<script type="text/javascript" charset="utf-8">
	function loadFriends() {
		var scriptTag = document.createElement("script");
		scriptTag.type = 'text/javascript';
		scriptTag.charset = "utf-8";
		scriptTag.src = "http://media1.friends.go.com/js/disney-friends-ostrta-min.js?v=1.3.27.1";
		document.getElementsByTagName("head")[0].appendChild(scriptTag);
	}
	
	if (window.attachEvent) {
		window.attachEvent('onload', loadFriends);
	} else {
		if (window.onload) {
			var windowOnLoad = window.onload;
			var replacedOnLoad = function() {
				windowOnLoad();
				loadFriends();
			};
			window.onload = replacedOnLoad;
		} else {
			window.onload = loadFriends;
		}
	}
</script>
